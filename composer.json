{
	"name": "nette/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"homepage": "http://nette.org",
	"type": "project",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "http://nette.org/contributors"
		}
	],
	"repositories": [{
		"type": "package",
		"package": {
			"name": "PetrP/Orm",
			"version": "v0.4",
			"dist": {
				"type": "zip",
				"url": "http://orm.petrprochazka.com/download/Orm-v0.4.0-RC6.zip"
			}
		}
	}],
	"require": {
		"php": ">= 5.3.0",
		"nette/nette": "@dev",
		"dg/adminer-custom": "1",
		"dg/dibi": "dev-master",
		"PetrP/Orm": "v0.4"
	},
    "require-dev": {
		"nette/tester": "@dev"
    },
	"scripts":
	{
		"post-install-cmd": "rm -rf ./vendor/PetrP/Orm/php52",
		"post-update-cmd": "rm -rf ./vendor/PetrP/Orm/php52"
	}
}
